# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/bin/
/logs/

# Ignore drupal code except for the custom modules and themes.
/html/*

!/html/modules/
/html/modules/*
!/html/modules/custom/

!/html/themes/
/html/themes/*
!/html/themes/custom/

!/html/sites/
/html/sites/*

!/html/sites/default/
/html/sites/default/*
!/html/sites/default/default.settings.local.php
!/html/sites/default/settings.docksal.php

# Ignore node modules.
/node_modules

# Ignore docker image build log.
buildlog.txt

# Ignore local dev directories.
/shared/
/database/
/private/
/notes/

